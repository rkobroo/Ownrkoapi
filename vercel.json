
{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api"
    },
    {
      "source": "/rko/alldl",
      "destination": "/api"
    },
    {
      "source": "/api/rko/alldl",
      "destination": "/api"
    },
    {
      "source": "/download/(.*)",
      "destination": "/api"
    },
    {
      "source": "/api/download/(.*)",
      "destination": "/api"
    }
  ]
}
