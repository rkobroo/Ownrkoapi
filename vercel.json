
{
  "version": 2,
  "functions": {
    "api/index.mjs": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/rko/alldl",
      "destination": "/api/index.mjs"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.mjs"
    },
    {
      "source": "/download/(.*)",
      "destination": "/api/index.mjs"
    }
  ]
}
